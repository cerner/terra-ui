{
  "name": "terra-ui",
  "private": true,
  "version": "0.0.0",
  "description": "terra-ui is the documentation site for the Terra UI ecosystem.",
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-ui/issues"
  },
  "engines": {
    "node": "^8.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-ui.git"
  },
  "scripts": {
    "clean": "node scripts/clean-modules/index.js",
    "clean:install": "npm run clean && npm install",
    "build:test": "NODE_ENV=test webpack --config webpack.config.js",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js -p",
    "build:development": "NODE_ENV=development webpack -w --config webpack.config.js",
    "generate-config": "generate-config -o 'dev-site-config' -s 'node_modules/terra-framework/packages/*/examples/*.site-page?(.jsx|.js)' -s 'node_modules/terra-core/packages/terra-site/src/examples/**/?(*Index|IconStatic|IconThemeable).jsx' -s 'node_modules/terra-clinical/packages/terra-clinical-site/src/examples/*/Index.jsx'",
    "heroku-postbuild": "npm install && npm run build:production",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .js,.jsx . --fix",
    "start": "tt:serve",
    "start-heroku": "tt:serve-static --site build",
    "test": "npm run lint",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^0.12.0",
    "glob": "^7.1.1",
    "lerna": "^2.7.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-particles-js": "^2.1.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.0",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.8",
    "stylelint": "^8.0.0",
    "stylelint-config-sass-guidelines": "^4.0.1",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.0.0",
    "stylelint-suitcss": "^1.0.0",
    "terra-clinical": "git://github.com/cerner/terra-clinical.git#terra-clinical-onset-picker@2.2.0",
    "terra-core": "git://github.com/cerner/terra-core.git#terra-toggle@2.5.0",
    "terra-dev-site": "git://github.com/cerner/terra-dev-site.git#webpack4",
    "terra-framework": "git://github.com/cerner/terra-framework.git#terra-theme-provider@2.2.0",
    "terra-grid": "^4.0.1",
    "terra-heading": "^2.0.1",
    "terra-i18n": "^2.0.0",
    "terra-image": "^2.0.1",
    "terra-text": "^2.0.1",
    "terra-toolkit": "^3.0.0",
    "xfc": "^1.2.1"
  }
}
