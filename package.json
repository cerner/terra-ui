{
  "name": "terra-ui",
  "private": true,
  "version": "0.0.0",
  "description": "terra-ui is the documentation site for the Terra UI ecosystem.",
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-ui/issues"
  },
  "engines": {
    "node": "^8.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-ui.git"
  },
  "scripts": {
    "clean": "node scripts/clean-modules/index.js",
    "clean:install": "npm run clean && npm install",
    "build:test": "NODE_ENV=test webpack --config webpack.config.js",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js -p",
    "build:development": "NODE_ENV=development webpack -w --config webpack.config.js",
    "heroku-postbuild": "npm install && npm run build:production",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .js,.jsx . --fix",
    "start": "tt-serve",
    "start-heroku": "tt-serve-static --site build",
    "test": "npm run lint",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.1.0",
    "glob": "^7.1.1",
    "lerna": "^2.7.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-particles-js": "^2.1.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.0",
    "rimraf": "^2.6.1",
    "shelljs": "^0.8.2",
    "stylelint": "^9.2.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.0",
    "stylelint-suitcss": "^3.0.0",
    "terra-clinical": "git://github.com/cerner/terra-clinical.git#terra-clinical-onset-picker@2.10.0",
    "terra-core": "git://github.com/cerner/terra-core.git#terra-tabs@3.10.0",
    "terra-dev-site": "^2.0.0",
    "terra-framework": "git://github.com/cerner/terra-framework.git#terra-dialog-modal@1.2.0",
    "terra-grid": "^4.0.1",
    "terra-heading": "^2.0.1",
    "terra-i18n": "^2.0.0",
    "terra-image": "^2.0.1",
    "terra-text": "^2.0.1",
    "terra-toolkit": "^4.2.0",
    "xfc": "^1.2.1"
  },
  "dependencies": {
    "terra-legacy-theme": "^2.0.0"
  }
}
