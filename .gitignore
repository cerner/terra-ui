# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
/db/*.sqlite3

/node_modules
/yarn-error.log

npm-debug.log*
node_modules

# Generated js bundles
/public/webpack/*

**/aggregated-translations/*.*
